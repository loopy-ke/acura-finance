{
    "name": "loopylabs/acura-finance",
    "description": "Credit and financing app for Acura One",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Loopy Labs",
            "email": "info@loopy.co.ke"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0",
        "spatie/laravel-permission": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "LoopyLabs\\CreditFinancing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LoopyLabs\\CreditFinancing\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LoopyLabs\\CreditFinancing\\CreditFinancingServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}